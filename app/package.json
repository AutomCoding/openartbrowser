{
  "name": "art-browser",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_docker": "ng serve --configuration=docker",
    "start:fr": "ng serve --base-href /fr/ --configuration=fr",
    "start:de": "ng serve --base-href /de/ --configuration=de",
    "start:en": "ng serve --base-href /en/ --configuration=en",
    "start:it": "ng serve --base-href /it/ --configuration=it",
    "start:es": "ng serve --base-href /es/ --configuration=es",
    "build": "ng build",
    "build-locale": "npm run build:en && npm run build:de && npm run build:fr && npm run build:es && npm run build:it",
    "build:fr": "ng build --base-href /fr/ --configuration=fr-production",
    "build:de": "ng build --base-href /de/ --configuration=de-production",
    "build:en": "ng build --base-href /en/ --configuration=en-production",
    "build:it": "ng build --base-href /it/ --configuration=it-production",
    "build:es": "ng build --base-href /es/ --configuration=es-production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "locale": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.10",
    "@angular/common": "^11.2.10",
    "@angular/compiler": "^11.2.10",
