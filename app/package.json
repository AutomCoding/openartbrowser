    "@angular/forms": "^11.2.10",
    "@angular/localize": "~11.2.10",
    "@angular/platform-browser": "^11.2.10",
    "@angular/platform-browser-dynamic": "^11.2.10",
    "@angular/router": "^11.2.10",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@types/element-resize-detector": "^1.1.2",
    "@types/lodash": "^4.14.168",
    "@types/qs": "^6.9.6",
    "angulartics2": "^8.2.0",
    "bodybuilder": "^2.3.0",
    "core-js": "^3.10.1",
    "element-resize-detector": "^1.2.1",
    "lodash": "^4.17.21",
    "ng-in-viewport": "^6.1.0",
    "ng5-slider": "^1.2.4",
    "ngx-fit-text": "0.0.10",
    "ngx-image-viewer": "github:hochschule-darmstadt/ngx-image-viewer#build",
    "rxjs": "^6.6.7",
